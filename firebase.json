{
  "hosting": {
    "public": "/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {  
          "serviceId": "gocook",
          "region": "us-central1",
          "target": "/"
        }
      } 
    ]
  }
}